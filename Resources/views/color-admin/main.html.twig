	<script>
		var handleIEFullHeightContent = function() {
			if ($.browser.msie) {
				$('.vertical-box-row [data-scrollbar="true"][data-height="100%"]').each(function() {
					var targetRow = $(this).closest('.vertical-box-row');
					$(targetRow).find('.vertical-box-cell').height('').height(targetRow.get(0).offsetHeight);
				});
			}
		};
		$(document).ready(function() {
			if ($.browser.msie && $.browser.version <= '9.0')
			{
				handlePageScrollClass = function(){};
				(function(){
					var hashchange = true;
					history.pushState = function(){
						hashchange = false;
						window.location.href = arguments[2];
					};

					$(window).on('hashchange', function(e) {
						if(!hashchange){
							e.stopImmediatePropagation();
							hashchange = true;
						}
					});
				})();
			}


			App.init({
				ajaxMode: true,
				ajaxDefaultUrl: '#pages/index.html',
				ajaxType: 'GET',
				ajaxDataType: 'html'
			});

			App.setPageOption({
				pageSidebarWide: true,
				//clearOptionOnLeave: true
			});
			if ($.browser.msie) {
				$(window).resize(handleIEFullHeightContent);
			}
